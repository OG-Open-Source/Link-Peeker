{
  "manifest_version": 3,
  "name": "Link Peeker",
  "version": "3.3.1.50",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "permissions": ["storage", "declarativeNetRequest"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_title": "Link Peeker"
  },
  "options_ui": {
    "page": "main.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "24": "assets/icons/icon24.png",
    "32": "assets/icons/icon32.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png",
    "256": "assets/icons/icon256.png",
    "512": "assets/icons/icon512.png",
    "1024": "assets/icons/icon1024.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; connect-src https: http:;"
  },
  "web_accessible_resources": [
    {
      "resources": ["assets/lib/gsap.min.js", "assets/lib/bulma.min.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
